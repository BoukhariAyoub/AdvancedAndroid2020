<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.boukharist.modernandroiddevelopment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyTheme.DayNight">
        <activity
            android:name=".screens.healthinfo.HealthInfoActivity"
            android:label="@string/title_activity_health_info"/>

        <activity
            android:name=".screens.registration.UserRegistrationActivity"
            android:label="@string/title_activity_user_regitrastion">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>